{
    "problemMatcher": [
        {
            "owner": "git-test-suite",
            "pattern": [
                {
                    "regexp": "^(.*)\\((error|warning|info):([^ :]+\\.sh):(\\d+)\\)$",
                    "file": 3,
                    "line": 4,
                    "severity": 2,
                    "message": 1
                }
            ]
        }
    ]
}
